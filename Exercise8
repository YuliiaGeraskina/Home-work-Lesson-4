package var2;
import java.util.Scanner;
import java.util.Arrays;
import java.util.StringTokenizer;
public class myclass25 {
	public static void main(String[] args) 
	throws java.io.IOException {
	Scanner scanner = new Scanner(System.in);
	System.out.print("Введите выражение 1+33-4*7:  ");
	String s=scanner.next();
	String a1=s.substring(0,1);
	String a2=s.substring(2,4);
	String a3=s.substring(5,6);
	String a4=s.substring(7);
	
	int a = Integer.parseInt(a1)+Integer.parseInt(a2)-Integer.parseInt(a3)*Integer.parseInt(a4);
	
	System.out.println("Результат:  " + a);
		
}
}
